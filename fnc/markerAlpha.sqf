/*
  Author: lukrop
  Date: 10/8/2013
  Description: Makes all mission related markers invisible
  by setting the marker alpha to zero.

  Parameters: -

  Returns: -

*/

{_x setMarkerAlpha 0} forEach [
"city0", "city1", "city2", "city3", "city4", "city5", "city6", "city7", "city8",
"land0", "land1", "land2", "land3", "land4", "land5", "land6", "land7", "land8", "land9", "land10", "land11", "land12", "land13", "land14", "land15"
];
